{
  "name": "canoe-scoreboard-v2",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "c123-proxy": "node scripts/c123-proxy.js",
    "mock:tcp": "npx tsx scripts/mock-c123-tcp.ts",
    "mock:ws": "npx tsx scripts/mock-cli-ws.ts",
    "test:providers": "vitest run src/__tests__/provider-comparison.test.ts",
    "test:visual": "playwright test cli-vs-c123.spec.ts --project=vertical"
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@fontsource/barlow-condensed": "^5.2.8",
    "@fontsource/bebas-neue": "^5.2.7",
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^17.3.0",
    "jsdom": "^28.0.0",
    "lighthouse": "^13.0.3",
    "tsx": "^4.19.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "ws": "^8.19.0"
  }
}
